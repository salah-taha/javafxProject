                        <Image url="@../images/exit.png" />
                    </image>
                </ImageView>
                <Label fx:id="id" layoutX="316.0" layoutY="27.0" text="id">
                    <font>
                        <Font name="Tajawal" size="16.0" />
                    </font>
                </Label>
                <ImageView fx:id="edit_btn" fitHeight="29.0" fitWidth="37.0" layoutX="66.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
